# .env.example

# Webhook Verification Token (used by Meta to verify the webhook URL)
VERIFY_TOKEN=

# Meta WhatsApp Cloud API Access Token
ACCESS_TOKEN=

# Your WhatsApp Business Phone Number ID (from Meta Dashboard)
PHONE_NUMBER_ID=

# Hugging Face or external API Key (if applicable)
API_KEY=

# OpenRouter API Key for generating responses
OPENROUTER_API_KEY=

# Referer URL (domain or IP from where requests are made, if required)
REFERER_URL=

# Project or Site name (used for prompts or logs)
SITE_NAME=

# OpenRouter model to use (e.g., google/gemini-2.5-flash-lite)
OPENROUTER_MODEL=


